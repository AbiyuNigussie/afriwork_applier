        "x-hasura-role": "job_seeker",
        "content-type": "application/json",
        "Sec-GPC": "1",
        "Sec-Fetch-Dest": "empty",
        "Sec-Fetch-Mode": "cors",
        "Sec-Fetch-Site": "same-site",
        "Priority": "u=0"
    },
    "referrer": "https://afriworket.com/",
    "body": "{\"operationName\":\"ApplyToJob\",\"query\":\"mutation ApplyToJob($application: JobApplicationInput!, $job_id: uuid!, $origin_platform_id: uuid!, $telegramUserName: String) {\\n  apply_to_job(\\n    application: $application\\n    job_id: $job_id\\n    origin_platform_id: $origin_platform_id\\n    telegram_username: $telegramUserName\\n  ) {\\n    application_id\\n    __typename\\n  }\\n}\",\"variables\":{\"application\":{\"cover_letter\":\"Dear Hiring Manager, \\n\\nI'm thrilled to take this opportunity|*|\"},\"job_id\":\"341e9fc5-983b-403d-8c18-abb7d0c6b470\",\"origin_platform_id\":\"9d325ee7-630f-44db-8f6c-d02b30947781\"}}",
